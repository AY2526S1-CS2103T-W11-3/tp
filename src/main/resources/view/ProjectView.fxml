<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>

<AnchorPane xmlns:fx="http://javafx.com/fxml">
    <VBox spacing="10" alignment="CENTER_LEFT" style="-fx-padding: 20;">
        <Label fx:id="name" style="-fx-font-size: 18; -fx-font-weight: bold;" maxWidth="200"/>
        <Label fx:id="deadline" wrapText="true" />
        <Label fx:id="priority" wrapText="true" />
        <Label fx:id="members" wrapText="true" />
    </VBox>
</AnchorPane>
