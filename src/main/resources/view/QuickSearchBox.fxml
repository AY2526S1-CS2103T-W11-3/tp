<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TextField?>

<!--
    QuickSearchBox.fxml
    A single-line TextField UI component that allows users to filter persons in real time.
    This file is paired with seedu.address.ui.QuickSearchBox.
-->

<TextField fx:id="quickSearchField"
           xmlns:fx="http://javafx.com/fxml/1"
           promptText="Quick search by name, phone, or email..."
           prefHeight="35"
           style="-fx-background-radius: 6; -fx-padding: 6 10; -fx-font-size: 13px;" />
